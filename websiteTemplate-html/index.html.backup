                <script defer="" data-name="footer-robotics">
                  document.addEventListener('DOMContentLoaded', function() {
                    (function(){
                      // Back to Top Functionality
                      const backToTopButton = document
                        .getElementById("footer-robotics")
                        .querySelector(".footer-back-to-top")

                      if (backToTopButton) {
                        backToTopButton.addEventListener("click", function () {
                          window.scrollTo({
                            top: 0,
                            behavior: "smooth",
                          })
                        })
                      }

                      // Add entrance animations on scroll
                      const footerObserver = new IntersectionObserver(
                        function (entries) {
                          entries.forEach(function (entry) {
                            if (entry.isIntersecting) {
                              entry.target.style.opacity = "1"
                              entry.target.style.transform = "translateY(0)"
                            }
                          })
                        },
                        {
                          threshold: 0.1,
                          rootMargin: "0px 0px -50px 0px",
                        }
                      )

                      const footerColumns = document.querySelectorAll(
                        ".footer-brand-column, .footer-links-column, .footer-contact-column"
                      )
                      footerColumns.forEach(function (column, index) {
                        column.style.opacity = "0"
                        column.style.transform = "translateY(30px)"
                        column.style.transition =
                          "opacity 0.6s ease-out " +
                          index * 0.1 +
                          "s, transform 0.6s ease-out " +
                          index * 0.1 +
                          "s"
                        footerObserver.observe(column)
                      })
                    })()
                  });
                </script>
              </div>
